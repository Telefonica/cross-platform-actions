{
  "name": "cross-platform-actions",
  "version": "1.0.0",
  "description": "Github action receiving a repo name, launching a deploy workflow there, waiting for the result, and returning the deploy manifest",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "test": "jest --config jest.config.js",
    "check:spelling": "cspell \"**/*\"",
    "check:types:test": "tsc --noEmit --project ./test/unit/tsconfig.json",
    "check:types:lib": "tsc --noEmit",
    "check:types": "npm run check:types:test && npm run check:types:lib",
    "build": "ncc build src/index.ts -o dist --source-map",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Telef√≥nica",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "1.10.0",
    "jest-environment-jsdom": "29.5.0",
    "jszip": "3.10.1",
    "octokit": "2.0.14",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.21.4",
    "@octokit/core": "4.2.0",
    "@octokit/rest": "19.0.7",
    "@octokit/types": "9.1.2",
    "@types/jest": "29.5.1",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@vercel/ncc": "0.36.1",
    "babel-jest": "29.5.0",
    "babel-plugin-module-resolver": "5.0.0",
    "cspell": "6.31.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "26.7.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "12.5.0",
    "prettier": "2.8.7",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "**/*.js": "eslint",
    "**/*.ts": "eslint",
    "**/*.json": "eslint",
    "**/*.*": "cspell --no-must-find-files"
  }
}
